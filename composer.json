{
    "name": "okipa/laravel-form-components",
    "description": "Ready-to-use and customizable form components.",
    "keywords": [
        "Okipa",
        "package",
        "laravel",
        "laravel-form-components",
        "php",
        "components",
        "generate",
        "generator",
        "generation",
        "html"
    ],
    "homepage": "https://github.com/okipa/laravel-form-components",
    "license": "MIT",
    "authors": [
        {
            "name": "Arthur LORENT",
            "email": "arthur.lorent@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.0",
        "phpstan/phpstan-mockery": "^0.12.14"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "nunomaduro/larastan": "^0.7",
        "orchestra/testbench": "^6.0",
        "phpmd/phpmd": "^2.9",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Okipa\\LaravelFormComponents\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Okipa\\LaravelFormComponents\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpcbf",
            "vendor/bin/phpcs",
            "vendor/bin/phpmd config,src,tests text phpmd.xml",
            "vendor/bin/phpstan analyse --memory-limit=2G",
            "vendor/bin/testbench package:test --parallel --no-coverage"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Okipa\\LaravelFormComponents\\LaravelFormComponentsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
